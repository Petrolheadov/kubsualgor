n = int(input("Enter the value of n: "))

# Create an n x n matrix filled with zeros
matrix = [[0 for _ in range(n)] for _ in range(n)]

# Fill the diagonals
for i in range(n):
    for j in range(n):
        matrix[i][j] = abs(i - j)

# Print the resulting matrix
for row in matrix:
    print(' '.join(map(str, row)))
