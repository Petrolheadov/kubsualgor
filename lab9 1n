def find_max_index(matrix):
    max_value = float('-inf')
    max_i, max_j = -1, -1
    
    for i in range(len(matrix)):
        for j in range(len(matrix[i])):
            if matrix[i][j] > max_value:
                max_value = matrix[i][j]
                max_i, max_j = i, j
    
    return max_i, max_j

# Введите размеры матрицы:
n = int(input("Введите количество рядов: "))
m = int(input("Введите количество столбцов: "))

# Инициализируйте матрицу
matrix = []
for i in range(n):
    row = list(map(int, input().split()))
    matrix.append(row)

# Найдите индексы максимального элемента
max_i, max_j = find_max_index(matrix)

# Вывод результата
print(f"Максимальный элемент находится на позиции ({max_i + 1}, {max_j + 1})")
